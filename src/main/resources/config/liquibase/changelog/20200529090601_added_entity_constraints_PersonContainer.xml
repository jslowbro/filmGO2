<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity PersonContainer.
    -->
    <changeSet id="20200529090601-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="person_container"
                                 constraintName="fk_person_container_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

        <addForeignKeyConstraint baseColumnNames="film_id"
                                 baseTableName="person_container"
                                 constraintName="fk_person_container_film_id"
                                 referencedColumnNames="id"
                                 referencedTableName="film"/>

    </changeSet>
</databaseChangeLog>
